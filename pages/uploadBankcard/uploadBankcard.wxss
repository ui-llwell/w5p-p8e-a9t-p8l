/* pages/uploadBankcard/uploadBankcard.wxss */
.center {
  text-align: center;
}
.content {
  width: 750rpx;
  height: 100%;
  background: #F5F5F5;
}

.content_textCard {
  font-size: 28rpx;
  color: #575E62;
  text-align: center;
  display: block;
  margin-bottom: 58rpx;
}
.content input {
  width: 460rpx;
  height: 76rpx;
  line-height: 76rpx;
  border-bottom: 1px solid #CED7DB;
  display: block;
  margin: 0 auto 22rpx auto;
  font-size: 34rpx;
}
.content_warp {
  width: 100%;
  height: 340rpx;
  position: relative;
  margin-top: 90rpx;
}
.content_warpBackground {
  width: 500rpx;
  height: 315rpx;
  margin: 0 auto;
  border: 1px dashed #CED7DB;
  border-radius: 30rpx;
  overflow: hidden;
}
.content_warpBackground image {
  width: 88rpx;
  height: 67rpx;
  display: block;
  margin: 124rpx auto 0 auto;
}
.content_warpBackground text {
  display: block;
  text-align: center;
  font-size: 22rpx;
  color: #909090;
  padding-top: 5rpx;
}
.content_warpCardImg {
  width: 500rpx;
  height: 315rpx;
  border-radius: 30rpx;
  position: absolute;
  left: 125rpx;
  top: 0rpx;
  border: 12rpx solid #C6C6C6;
  

}
.content_warpCardImg image {
  width: 500rpx;
  height: 305rpx;
  display: block;
  margin: 5rpx auto 0 auto;
}
.content_next {
  width: 404rpx;
  height: 106rpx;
  line-height: 106rpx;
  display: block;
  margin: 0 auto;
  font-size: 52rpx;
  color: #fff;
  background: #fa585d;
  text-align: center;
  border-radius: 50rpx;
  margin-top: 12rpx;
}
.content_skip {
  font-size: 24rpx;
  color: #0095FF;
  text-align: center;
  margin-top: 15rpx;
}
.content_warpCardImgCamera {
  width: 88rpx;
  height: 67rpx;
  position: absolute;
  left:199rpx;
  top:110rpx;

}
.content_warpCardImgCamera image {
 width: 88rpx;
 height: 67rpx; 
}