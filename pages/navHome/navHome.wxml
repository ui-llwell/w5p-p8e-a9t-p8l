<!--pages/navHome/navHome.wxml-->
<!-- <text>首页</text> -->

<view class='swiper'>
    <swiper indicator-dots="{{indicatorDots}}" vertical="{{vertical}}" autoplay="{{autoplay}}" duration="{{duration}}" interval='{{interval}}' bindchange="bindchange"  circular="{{circular}}" style="height:{{imgheights[current]}}rpx;">
    <block wx:for='{{imgList}}' wx:key="{{index}}">
      <swiper-item>
        <image src="{{item.image}}" data-id='{{index}}' class="slide-image" mode="widthFix" bindload="imageLoad"/>
        <view class='swiper_text'>
          <text class='swiper_textIntroduce'>{{item.text1}}</text>
          <text class='swiper_textIntroduce'>{{item.text2}}</text>
          <text class='swiper_textLink'>{{item.text3}}</text>
        </view>
      </swiper-item>
      </block>
    </swiper>
  </view>
  <view class='index'>
    <view class='main'>
      <view class='f1 main_consumption' ><image src='http://llwell-wxapp.oss-cn-beijing.aliyuncs.com/PurchasingAssistantPersonal/top_img_consumption@3x.png' bindtap='consumption'></image></view>
      <view class='r1 main_SmallTicket'><image src='http://llwell-wxapp.oss-cn-beijing.aliyuncs.com/PurchasingAssistantPersonal/top_img_receipt@3x.png' bindtap='SmallTicket'></image></view>
    </view>

    <view class='nav'>
      <view class='nav_text'>
        <text class='nav_ReturnPoint'>流连合作免税店最新返点</text>
        <text class='nav_time'>2018.9.01</text>
      </view>
    </view>
    <view class='table' wx:for='{{List}}' wx:key="{{id}}">
      <view class='table_left f1'>
        <text class='table_leftText1'>{{item.text1}}</text>
        <text class='table_leftText2'>{{item.text2}}</text>
      </view>
      <view class='table_right r1'>
        <text class='table_rightText1'>{{item.text3}}</text>
        <text class='table_rightText2'>{{item.text4}}</text>
      </view>
    </view>
    <view class='footer'>
      <text class='footer_wcome'>WELCOME</text>
      <text class='footer_link' bindtap='CooperativeShop'>查看合作店铺	></text>
    </view>
    
  </view>