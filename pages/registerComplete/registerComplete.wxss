/* pages/registerComplete/registerComplete.wxss */

.index {
  width: 750rpx;
  height: 100%;
  background: #F5F5F5;
}
.container {
  width: 750rpx;
  height: 100%;
  
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 384rpx;
}

.userinfo-avatar {
  width: 171.5rpx;
  height:  171.5rpx;
  border-radius: 50%;
  border:28.5rpx solid #cdebed;
  margin-top: 45rpx;
}

.userinfo-nickname {
  color: #232627;
  font-size: 40rpx;
  margin-top: 30rpx;
  
}
.index_text {
  display: block;
  text-align: center;
  font-size: 26rpx;
  color: #575E62;
}
.content_h1 {
  text-align: center;
  font-size: 34rpx;
  color: #232627;
  margin-bottom: 58rpx;
}
.content_clear {
  clear: both;
  width: 750rpx;
  height: 63rpx;
}
.content_card {
  display: flex;
  margin-top: 63rpx;
  
}
.content_card view {
  width: 280rpx;
  /* height: 240rpx; */
  flex: 1;
}
.content_card view image {
  display: block;
  width: 280rpx;
  height: 176.4rpx;
  margin: 0 auto;
  border-radius: 20rpx;
}
.content_cardPassport {
  border-right: 1px dashed #979797;
  height: 191rpx;
}
.content_card view text {
  text-align: center;
  display: block;
  margin-top: 25rpx;
  margin-bottom: 116rpx;
}
.content_start {
  width:404rpx;
  height:106rpx;
  line-height:106rpx;
  display:block;
  margin:0 auto;
  font-size:52rpx;
  color:#fff;
  background:#fa585d;
  text-align:center;
  border-radius:50rpx;
  margin-bottom: 104rpx;
}